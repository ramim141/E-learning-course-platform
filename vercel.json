{
    "builds": [
      {
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "core/wsgi.py"
      }
    ]
  }